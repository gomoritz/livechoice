{
   "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "Node",
      "target": "es2015",
      "declaration": true,
      "sourceMap": false,
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "noImplicitThis": true,
      "strictPropertyInitialization": false,
      "rootDir": "src",
      "outDir": "dist",
      "types": ["node"],
      "typeRoots": []
   }
}
